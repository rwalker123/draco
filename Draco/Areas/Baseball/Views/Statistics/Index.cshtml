@using System.Web.Optimization
@using SportsManager.Baseball.ViewModels;
@model SeasonStatisticsViewModel

@{
	ViewBag.Title = @Model.AccountName + " Season Statistics";
}

@section head {
@Styles.Render("~/Content/typeahead")
}

<div class="jumbotron">
    @Header.DisplayHeader("Season Statistics", Model.AccountName, Model.AccountLogoUrl)
</div>

<div class="container-fluid" id="statisticsView">
    <div class="row">
        <div class="col-lg-12">
            <div class="input-group">
                <input type="text" class="form-control pull-right" placeholder="find player (type last name)" data-bind="typeahead: { datasets: { source: getPlayers, displayKey: 'label' }, target: foundPlayer, options: { highlight: true } }" />
                <span class="input-group-btn">
                    <button class="btn btn-default" type="button" title="view player" data-bind="click: loadPlayer"><span class="glyphicon glyphicon-eye-open"></span></button>
                </span>
            </div>
        </div>
    </div>
    <div style="margin-top: 10px" class="row">
        <div class="col-lg-12 col-md-12 col-sm-12 text-center">
            <label class="control-label">Season:</label>
            <select data-container="body" data-bind="selectPicker: selectedSeasonId">
                @foreach (var season in Model.GetSeasons())
                {
                    <option value="@season.Value">@season.Text</option>
                }
            </select>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12">
            <ul class="nav nav-tabs">
                <li class="active"><a href="#seasonstats" data-toggle="tab">League Stats</a></li>
                <li><a href="#teamstats" data-toggle="tab">Team Stats</a></li>
                <li><a href="#seasonstandings" data-toggle="tab">Standings</a></li>
            </ul>

            <div class="tab-content">
                <div class="tab-pane fade in active" id="seasonstats">
                    <form class="form-horizontal" style="margin-top: 20px">
                        <div class="form-group">
                            <label class="col-lg-1 col-sm-2 control-label">League: </label>
                            <div class="col-lg-4 col-sm-4">
                                <select data-container="body" data-bind="selectPicker: selectedLeagueId, selectPickerOptions: { optionsArray: leagues }, optionsText: 'Name', optionsValue: 'Id'"></select>
                            </div>
                            <label class="col-lg-1 col-sm-2 control-label">Division:</label>
                            <div class="col-lg-4 col-sm-4">
                                <select data-container="body" data-bind="selectPicker: selectedDivisionId, selectPickerOptions: { optionsArray: divisions }, optionsText: 'Name', optionsValue: 'Id'"></select>
                            </div>
                        </div>
                    </form>
                    <ul class="nav nav-pills">
                        <li class="active"><a href="#leaders" data-toggle="tab">Leaders</a></li>
                        <li><a href="#batsortablestats" data-toggle="tab">Batting Sortable Stats</a></li>
                        <li><a href="#pitchsortablestats" data-toggle="tab">Pitching Sortable Stats</a></li>
                    </ul>
                    <div class="tab-content">
                        <div class="tab-pane fade in active" id="leaders">
                            <h3>Hitting Leaders</h3>
                            <div class="row" data-bind="foreach: leaders.batCategories">
                                <div class="col-md-4">
                                    <div class="table-responsive" style="margin: 5px">
                                        <table class="table table-striped table-bordered">
                                            <thead>
                                                <tr class="bg-primary">
                                                    <th colspan="4" class="text-center" data-bind="html: Name"></th>
                                                </tr>
                                            </thead>
                                            <tbody data-bind="foreach: Leaders">
                                                <tr>
                                                    <!-- ko if: FieldName == "TIE" -->
                                                    <td></td>
                                                    <td data-bind="html: FieldName"></td>
                                                    <td><span data-bind="html: TieCount"></span> Players</td>
                                                    <!-- /ko -->
                                                    <!-- ko if: FieldName != "TIE" -->
                                                    <td><img onerror="this.style.display = 'none';" data-bind='attr: { src: PlayerName.PhotoURL }' class="img-rounded" /></td>
                                                    <td>
                                                        <!-- ko if: $root.selectedSeasonId() > 0 -->
                                                        <a data-bind="trimText: PlayerName.LastName, trimTextLength: 10, attr: { title: PlayerName.FirstName + ' ' + PlayerName.LastName, href: window.config.rootUri + '/baseball/player/season/@Model.AccountId/' + PlayerId }"></a>
                                                        <!-- /ko -->
                                                        <!-- ko if: $root.selectedSeasonId() == 0 -->
                                                        <a data-bind="trimText: PlayerName.LastName, trimTextLength: 10, attr: { title: PlayerName.FirstName + ' ' + PlayerName.LastName, href: window.config.rootUri + '/baseball/player/roster/@Model.AccountId/' + PlayerId }"></a>
                                                        <!-- /ko -->
                                                    </td>
                                                    <td><a data-bind="trimText: TeamName, trimTextLength: 10, attr: { title: TeamName, href: window.config.rootUri + '/Baseball/Team/Index/@Model.AccountId/' + TeamId }"></a></td>
                                                    <!-- /ko -->
                                                    <td data-bind="html: FieldTotal"></td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>

                                </div>
                            </div>
                            <h3>Pitching Leaders</h3>
                            <div class="row" data-bind="foreach: leaders.pitchCategories">
                                <div class="col-md-4">
                                    <div class="table-responsive" style="margin: 5px">
                                        <table class="table table-striped table-bordered">
                                            <thead>
                                                <tr class="bg-primary">
                                                    <th colspan="4" class="text-center" data-bind="html: Name"></th>
                                                </tr>
                                            </thead>
                                            <tbody data-bind="foreach: Leaders">
                                                <tr>
                                                    <!-- ko if: FieldName == "TIE" -->
                                                    <td></td>
                                                    <td data-bind="html: FieldName"></td>
                                                    <td><span data-bind="html: TieCount"></span> Players</td>
                                                    <!-- /ko -->
                                                    <!-- ko if: FieldName != "TIE" -->
                                                    <td><img onerror="this.style.display = 'none';" data-bind='attr: { src: PlayerName.PhotoURL }' class="img-rounded" /></td>
                                                    <td>
                                                        <!-- ko if: $root.selectedSeasonId() > 0 -->
                                                        <a data-bind="trimText: PlayerName.LastName, trimTextLength: 10, attr: { title: PlayerName.FirstName + ' ' + PlayerName.LastName, href: window.config.rootUri + '/baseball/player/season/@Model.AccountId/' + PlayerId }"></a>
                                                        <!-- /ko -->
                                                        <!-- ko if: $root.selectedSeasonId() == 0 -->
                                                        <a data-bind="trimText: PlayerName.LastName, trimTextLength: 10, attr: { title: PlayerName.FirstName + ' ' + PlayerName.LastName, href: window.config.rootUri + '/baseball/player/roster/@Model.AccountId/' + PlayerId }"></a>
                                                        <!-- /ko -->
                                                    </td>
                                                    <td><a data-bind="trimText: TeamName, trimTextLength: 10, attr: { title: TeamName, href: window.config.rootUri + '/Baseball/Team/Index/@Model.AccountId/' + TeamId }"></a></td>
                                                    <!-- /ko -->
                                                    <td data-bind="html: FieldTotal"></td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="tab-pane fade" id="batsortablestats">
                            <ul class="pager">
                                <li class="previous"><a data-bind="visible: prevBatStatsAvailable, click: gotoBatStatsPrev">&larr; Previous</a></li>
                                <li class="next"><a data-bind="visible: nextBatStatsAvailable, click: gotoBatStatsNext">Next &rarr;</a></li>
                            </ul>

                            <div class="table-responsive">
                                <table class="table table-striped statsTable">
                                    <thead>
                                        <tr>
                                            <th>Name</th>
                                            <th class="sortableField" data-bind="click: batSortBy" data-toggle="tooltip" title="at bats">AB</th>
                                            <th class="sortableField" data-bind="click: batSortBy" data-toggle="tooltip" title="hits">H</th>
                                            <th class="sortableField" data-bind="click: batSortBy" data-toggle="tooltip" title="runs scored">R</th>
                                            <th class="sortableField" data-bind="click: batSortBy" data-toggle="tooltip" title="doubles" data-sort-field="D">2B</th>
                                            <th class="sortableField" data-bind="click: batSortBy" data-toggle="tooltip" title="triples" data-sort-field="T">3B</th>
                                            <th class="sortableField" data-bind="click: batSortBy" data-toggle="tooltip" title="home runs">HR</th>
                                            <th class="sortableField" data-bind="click: batSortBy" data-toggle="tooltip" title="runs batted in">RBI</th>
                                            <th class="sortableField" data-bind="click: batSortBy" data-toggle="tooltip" title="strike outs">SO</th>
                                            <th class="sortableField" data-bind="click: batSortBy" data-toggle="tooltip" title="walks">BB</th>
                                            <th class="sortableField" data-bind="click: batSortBy" data-toggle="tooltip" title="hit by pitch">HBP</th>
                                            <th class="sortableField" data-bind="click: batSortBy" data-toggle="tooltip" title="stolen base">SB</th>
                                            <th class="sortableField" data-bind="click: batSortBy" data-toggle="tooltip" title="total bases">TB</th>
                                            <th class="sortableField" data-bind="click: batSortBy" data-toggle="tooltip" title="plate appearances">PA</th>
                                            <th class="sortableField" data-bind="click: batSortBy" data-toggle="tooltip" title="on base percentage" data-sort-field="OBA">OBP</th>
                                            <th class="sortableField" data-bind="click: batSortBy" data-toggle="tooltip" title="slugging percentage">SLG</th>
                                            <th class="sortableField" data-bind="click: batSortBy" data-toggle="tooltip" title="on base percentage + slugging percentage">OPS</th>
                                            <th class="sortableField" data-bind="click: batSortBy" data-toggle="tooltip" title="batting average">AVG</th>
                                        </tr>
                                    </thead>
                                    <tbody data-bind="foreach: leagueSortableBatStats">
                                        <tr>
                                            <!-- ko if: $root.selectedSeasonId() > 0 -->
                                            <td><a data-bind="html: PlayerName, attr: { href: window.config.rootUri + '/baseball/player/season/@Model.AccountId/' + PlayerId }"></a></td>
                                            <!-- /ko -->
                                            <!-- ko if: $root.selectedSeasonId() == 0 -->
                                            <td><a data-bind="html: PlayerName, attr: { href: window.config.rootUri + '/baseball/player/roster/@Model.AccountId/' + PlayerId }"></a></td>
                                            <!-- /ko -->
                                            <td data-bind="html: AB"></td>
                                            <td data-bind="html: H"></td>
                                            <td data-bind="html: R"></td>
                                            <td data-bind="html: D"></td>
                                            <td data-bind="html: T"></td>
                                            <td data-bind="html: HR"></td>
                                            <td data-bind="html: RBI"></td>
                                            <td data-bind="html: SO"></td>
                                            <td data-bind="html: BB"></td>
                                            <td data-bind="html: HBP"></td>
                                            <td data-bind="html: SB"></td>
                                            <td data-bind="html: TB"></td>
                                            <td data-bind="html: PA"></td>
                                            <td data-bind="numericText: OBA, precision: 3, removeLeadingZero: true"></td>
                                            <td data-bind="numericText: SLG, precision: 3"></td>
                                            <td data-bind="numericText: OPS, precision: 3"></td>
                                            <td data-bind="numericText: AVG, precision: 3, removeLeadingZero: true"></td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                        <div class="tab-pane fade" id="pitchsortablestats">
                            <ul class="pager">
                                <li class="previous"><a data-bind="visible: prevPitchStatsAvailable, click: gotoPitchStatsPrev">&larr; Previous</a></li>
                                <li class="next"><a data-bind="visible: nextPitchStatsAvailable, click: gotoPitchStatsNext">Next &rarr;</a></li>
                            </ul>

                            <div class="table-responsive">
                                <table id="pitchStatsTable" class="table table-striped statsTable">
                                    <thead>
                                        <tr>
                                            <th>Name</th>
                                            <th class="sortableField" data-bind="click: pitchSortBy" data-toggle="tooltip" title="wins">W</th>
                                            <th class="sortableField" data-bind="click: pitchSortBy" data-toggle="tooltip" title="losses">L</th>
                                            <th class="sortableField" data-bind="click: pitchSortBy" data-toggle="tooltip" title="saves">S</th>
                                            <th class="sortableField" data-bind="click: pitchSortBy" data-toggle="tooltip" title="innings pitched">IP</th>
                                            <th class="sortableField" data-bind="click: pitchSortBy" data-toggle="tooltip" title="batters faced">BF</th>
                                            <th class="sortableField" data-bind="click: pitchSortBy" data-toggle="tooltip" title="hits given up">H</th>
                                            <th class="sortableField" data-bind="click: pitchSortBy" data-toggle="tooltip" title="runs scored">R</th>
                                            <th class="sortableField" data-bind="click: pitchSortBy" data-toggle="tooltip" title="earned runs scored">ER</th>
                                            <th class="sortableField" data-bind="click: pitchSortBy" data-toggle="tooltip" title="doubles" data-sort-field="D">2B</th>
                                            <th class="sortableField" data-bind="click: pitchSortBy" data-toggle="tooltip" title="triples" data-sort-field="T">3B</th>
                                            <th class="sortableField" data-bind="click: pitchSortBy" data-toggle="tooltip" title="home runs">HR</th>
                                            <th class="sortableField" data-bind="click: pitchSortBy" data-toggle="tooltip" title="strike outs">SO</th>
                                            <th class="sortableField" data-bind="click: pitchSortBy" data-toggle="tooltip" title="walks">BB</th>
                                            <th class="sortableField" data-bind="click: pitchSortBy" data-toggle="tooltip" title="hit batters" data-sort-field="HBP">HB</th>
                                            <th class="sortableField" data-bind="click: pitchSortBy" data-toggle="tooltip" title="strikouts per 9 innings" data-sort-field="K9">K/9</th>
                                            <th class="sortableField" data-bind="click: pitchSortBy" data-toggle="tooltip" title="walks per 9 innings" data-sort-field="BB9">BB/9</th>
                                            <th class="sortableField" data-bind="click: pitchSortBy" data-toggle="tooltip" title="opponents batting average">OBA</th>
                                            <th class="sortableField" data-bind="click: pitchSortBy" data-toggle="tooltip" title="opponents slugging percentage">SLG</th>
                                            <th class="sortableField" data-bind="click: pitchSortBy" data-toggle="tooltip" title="walks + hits / innings pitched">WHIP</th>
                                            <th class="sortableField" data-bind="click: pitchSortBy" data-toggle="tooltip" title="earned run average">ERA</th>
                                        </tr>
                                    </thead>
                                    <tbody data-bind="foreach: leagueSortablePitchStats">
                                        <tr>
                                            <!-- ko if: $root.selectedSeasonId() > 0 -->
                                            <td><a data-bind="html: PlayerName, attr: { href: window.config.rootUri + '/baseball/player/season/@Model.AccountId/' + PlayerId }"></a></td>
                                            <!-- /ko -->
                                            <!-- ko if: $root.selectedSeasonId() == 0 -->
                                            <td><a data-bind="html: PlayerName, attr: { href: window.config.rootUri + '/baseball/player/roster/@Model.AccountId/' + PlayerId }"></a></td>
                                            <!-- /ko -->
                                            <td data-bind="html: W"></td>
                                            <td data-bind="html: L"></td>
                                            <td data-bind="html: S"></td>
                                            <td data-bind="html: IPDecimal"></td>
                                            <td data-bind="html: BF"></td>
                                            <td data-bind="html: H"></td>
                                            <td data-bind="html: R"></td>
                                            <td data-bind="html: ER"></td>
                                            <td data-bind="html: D"></td>
                                            <td data-bind="html: T"></td>
                                            <td data-bind="html: HR"></td>
                                            <td data-bind="html: SO"></td>
                                            <td data-bind="html: BB"></td>
                                            <td data-bind="html: HBP"></td>
                                            <td data-bind="numericText: K9, precision: 1"></td>
                                            <td data-bind="numericText: BB9, precision: 1"></td>
                                            <td data-bind="numericText: OBA, precision: 3, removeLeadingZero: true"></td>
                                            <td data-bind="numericText: SLG, precision: 3"></td>
                                            <td data-bind="numericText: WHIP, precision: 2"></td>
                                            <td data-bind="numericText: ERA, precision: 2"></td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="tab-pane fade" id="teamstats">
                    <form class="form-horizontal" style="margin-top: 20px">
                        <div class="form-group">
                            <label class="col-lg-1 col-sm-2 control-label">Team: </label>
                            <div class="col-lg-4 col-sm-4">
                                <select data-container="body" data-bind="selectPicker: selectedTeamId, selectPickerOptions: { optionsArray: teamsList }, optionsText: 'Name', optionsValue: 'Id', optionsCaption: 'select team...'"></select>
                            </div>
                        </div>
                    </form>

                    <ul class="nav nav-pills">
                        <li class="active"><a href="#batteamstats" data-toggle="tab">Batting</a></li>
                        <li><a href="#pitchteamstats" data-toggle="tab">Pitching</a></li>
                    </ul>

                    <div class="tab-content">
                        <div id="batteamstats" class="tab-pane fade in active">
                            <div class="table-responsive">
                                <table class="table table-striped statsTable">
                                    <thead>
                                        <tr>
                                            <th>Name</th>
                                            <th data-toggle="tooltip" title="at bats">AB</th>
                                            <th data-toggle="tooltip" title="hits">H</th>
                                            <th data-toggle="tooltip" title="runs scored">R</th>
                                            <th data-toggle="tooltip" title="doubles" data-sort-field="D">2B</th>
                                            <th data-toggle="tooltip" title="triples" data-sort-field="T">3B</th>
                                            <th data-toggle="tooltip" title="home runs">HR</th>
                                            <th data-toggle="tooltip" title="runs batted in">RBI</th>
                                            <th data-toggle="tooltip" title="strike outs">SO</th>
                                            <th data-toggle="tooltip" title="walks">BB</th>
                                            <th data-toggle="tooltip" title="hit by pitch">HBP</th>
                                            <th data-toggle="tooltip" title="stolen base">SB</th>
                                            <th data-toggle="tooltip" title="total bases">TB</th>
                                            <th data-toggle="tooltip" title="plate appearances">PA</th>
                                            <th data-toggle="tooltip" title="on base percentage" data-sort-field="OBA">OBP</th>
                                            <th data-toggle="tooltip" title="slugging percentage">SLG</th>
                                            <th data-toggle="tooltip" title="on base percentage + slugging percentage">OPS</th>
                                            <th data-toggle="tooltip" title="batting average">AVG</th>
                                        </tr>
                                    </thead>
                                    <tbody data-bind="foreach: teamSortableBatStats">
                                        <tr>
                                            <td data-bind="html: PlayerName"></td>
                                            <td data-bind="html: AB"></td>
                                            <td data-bind="html: H"></td>
                                            <td data-bind="html: R"></td>
                                            <td data-bind="html: D"></td>
                                            <td data-bind="html: T"></td>
                                            <td data-bind="html: HR"></td>
                                            <td data-bind="html: RBI"></td>
                                            <td data-bind="html: SO"></td>
                                            <td data-bind="html: BB"></td>
                                            <td data-bind="html: HBP"></td>
                                            <td data-bind="html: SB"></td>
                                            <td data-bind="html: TB"></td>
                                            <td data-bind="html: PA"></td>
                                            <td data-bind="numericText: OBA, precision: 3, removeLeadingZero: true"></td>
                                            <td data-bind="numericText: SLG, precision: 3"></td>
                                            <td data-bind="numericText: OPS, precision: 3"></td>
                                            <td data-bind="numericText: AVG, precision: 3, removeLeadingZero: true"></td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                        <div id="pitchteamstats" class="tab-pane fade">
                            <div class="table-responsive">
                                <table id="pitchStatsTable" class="table table-striped statsTable">
                                    <thead>
                                        <tr>
                                            <th>Name</th>
                                            <th data-toggle="tooltip" title="wins">W</th>
                                            <th data-toggle="tooltip" title="losses">L</th>
                                            <th data-toggle="tooltip" title="saves">S</th>
                                            <th data-toggle="tooltip" title="innings pitched">IP</th>
                                            <th data-toggle="tooltip" title="batters faced">BF</th>
                                            <th data-toggle="tooltip" title="hits given up">H</th>
                                            <th data-toggle="tooltip" title="runs scored">R</th>
                                            <th data-toggle="tooltip" title="earned runs scored">ER</th>
                                            <th data-toggle="tooltip" title="doubles" data-sort-field="D">2B</th>
                                            <th data-toggle="tooltip" title="triples" data-sort-field="T">3B</th>
                                            <th data-toggle="tooltip" title="home runs">HR</th>
                                            <th data-toggle="tooltip" title="strike outs">SO</th>
                                            <th data-toggle="tooltip" title="walks">BB</th>
                                            <th data-toggle="tooltip" title="hit batters" data-sort-field="HBP">HB</th>
                                            <th data-toggle="tooltip" title="strikouts per 9 innings" data-sort-field="K9">K/9</th>
                                            <th data-toggle="tooltip" title="walks per 9 innings" data-sort-field="BB9">BB/9</th>
                                            <th data-toggle="tooltip" title="opponents batting average">OBA</th>
                                            <th data-toggle="tooltip" title="opponents slugging percentage">SLG</th>
                                            <th data-toggle="tooltip" title="walks + hits / innings pitched">WHIP</th>
                                            <th data-toggle="tooltip" title="earned run average">ERA</th>
                                        </tr>
                                    </thead>
                                    <tbody data-bind="foreach: teamSortablePitchStats">
                                        <tr>
                                            <td data-bind="html: PlayerName"></td>
                                            <td data-bind="html: W"></td>
                                            <td data-bind="html: L"></td>
                                            <td data-bind="html: S"></td>
                                            <td data-bind="html: IPDecimal"></td>
                                            <td data-bind="html: BF"></td>
                                            <td data-bind="html: H"></td>
                                            <td data-bind="html: R"></td>
                                            <td data-bind="html: ER"></td>
                                            <td data-bind="html: D"></td>
                                            <td data-bind="html: T"></td>
                                            <td data-bind="html: HR"></td>
                                            <td data-bind="html: SO"></td>
                                            <td data-bind="html: BB"></td>
                                            <td data-bind="html: HBP"></td>
                                            <td data-bind="numericText: K9, precision: 1"></td>
                                            <td data-bind="numericText: BB9, precision: 1"></td>
                                            <td data-bind="numericText: OBA, precision: 3, removeLeadingZero: true"></td>
                                            <td data-bind="numericText: SLG, precision: 3"></td>
                                            <td data-bind="numericText: WHIP, precision: 2"></td>
                                            <td data-bind="numericText: ERA, precision: 2"></td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>

                </div>
                <div class="tab-pane fade" id="seasonstandings">
                    <!-- ko if: selectedSeasonId() == 0 -->
                    <div class="alert alert-info">
                        Not available for "All Seasons". Please select and individual season.
                    </div>
                    <!-- /ko -->
                    <!-- ko if: selectedSeasonId() > 0 && standingLeagues().length == 0 -->
                    <div class="alert alert-info">
                        No standings found for selected season.
                    </div>
                    <!-- /ko -->
                    <div data-bind="foreach: standingLeagues">
                        <h2 data-bind="html: Name"></h2>
                        <div data-bind="foreach: Divisions">
                            <div class="row">
                                <div class="col-md-8 col-sm-12 col-lg-6">
                                    <table class="table table-striped table-bordered">
                                        <thead>
                                            <tr>
                                                <th data-bind="html: Name"></th>
                                                <th>&nbsp;W&nbsp;</th>
                                                <th>&nbsp;L&nbsp;</th>
                                                <th>&nbsp;T&nbsp;</th>
                                                <th>&nbsp;Pct&nbsp;</th>
                                                <th>&nbsp;GB&nbsp;</th>
                                                <th>Div</th>
                                            </tr>
                                        </thead>
                                        <tbody data-bind="foreach: Standings">
                                            <tr>
                                                <td style="overflow: hidden; white-space: nowrap;">
                                                    <a data-bind="html: TeamName, attr: { href: window.config.rootUri + '/Baseball/Team/Index/@Model.AccountId/' + TeamId }"></a>
                                                </td>
                                                <td data-bind="html: Wins"></td>
                                                <td data-bind="html: Losses"></td>
                                                <td data-bind="html: Ties"></td>
                                                <td data-bind="numericText: WinningPct, precision: 3, removeLeadingZero: true"></td>
                                                <td data-bind="numericText: GamesBack, precision: 1"></td>
                                                <td><span data-bind="html: DivisionWins"></span>-<span data-bind="html: DivisionLosses"></span>-<span data-bind="html: DivisionTies"></span></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
