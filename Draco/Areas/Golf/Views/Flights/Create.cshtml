@model SportsManager.Golf.ViewModels.Controllers.FlightViewModel

@{
	ViewBag.Title = ViewData["Title"];
}

<h1>@ViewBag.Title</h1>

@{
    Html.EnableClientValidation();   
}

@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()

    @Html.EditorForModel()
                
    @Html.ValidationSummary()
    <input type="submit" value="Save" />
    @Html.ActionLink("Cancel", "Index", "Flights", new { accountId = @Model.AccountId, id = @Model.SeasonId }, null)
}

