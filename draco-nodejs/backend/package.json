{
  "name": "@draco/backend",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "start": "node dist/src/server.js",
    "dev": "nodemon --exec npm start",
    "build": "prisma generate && tsc",
    "fix-esm-imports": "node ./scripts/fix-esm-imports.mjs",
    "clean": "rm -rf dist",
    "migrate-passwords": "ts-node scripts/migrate-passwords.ts",
    "test-passwords": "ts-node scripts/test-password-verification.ts",
    "docs:generate": "node scripts/generate-docs.mjs",
    "prepare": "husky",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "lint:fix": "eslint 'src/**/*.{js,ts}' --fix",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit",
    "build:lambda": "npm run build && cd dist && zip -r ../draco-backend.zip . && cd ..",
    "generate:openapi": "tsx scripts/generate-openapi.ts",
    "generate:client": "openapi-ts --input ./openapi.json --output ../shared/shared-api-client/generated -c @hey-api/client-next"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^8.1.0",
    "@aws-sdk/client-s3": "^3.540.0",
    "@aws-sdk/s3-request-presigner": "^3.540.0",
    "@draco/shared-schemas": "*",
    "@mui/x-date-pickers": "^8.6.0",
    "@prisma/client": "^6.14.0",
    "@types/express": "^5.0.3",
    "@types/multer": "^1.4.13",
    "@types/nodemailer": "^6.4.17",
    "@types/sharp": "^0.31.1",
    "@types/validator": "^13.15.3",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.0.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.1",
    "nodemailer": "^7.0.6",
    "on-finished": "^2.4.1",
    "sharp": "^0.34.3",
    "swagger-ui-express": "^5.0.0",
    "validator": "^13.15.15",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.0",
    "@types/aws-lambda": "^8.10.152",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/dotenv": "^6.1.1",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "node-mocks-http": "^1.17.2",
    "nodemon": "^3.1.10",
    "prisma": "^6.14.0",
    "supertest": "^7.1.1",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^8.39.1",
    "tsx": "^4.19.2",
    "@hey-api/openapi-ts": "^0.83.1"
  }
}
