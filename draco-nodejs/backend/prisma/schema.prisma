  created_by            aspnetusers?        @relation("EmailCreatedBy", fields: [created_by_user_id], references: [id])
  template              email_templates?    @relation(fields: [template_id], references: [id])
  sender_contact        contacts?           @relation("EmailSenderContact", fields: [sender_contact_id], references: [id], onDelete: SetNull)

  @@index([account_id, status])
  @@index([scheduled_send_at])
  @@index([sender_contact_id])
