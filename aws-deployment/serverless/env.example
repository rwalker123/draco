# Draco Serverless Deployment Environment Variables
# Copy this file to .env and update with your actual values

# Database Configuration
TF_VAR_db_password=your-secure-password-here

# JWT Configuration
TF_VAR_jwt_secret=your-super-secret-jwt-key-here

# Email Configuration
TF_VAR_email_user=your-email@example.com
TF_VAR_email_pass=your-email-password-here

# Optional Email Server Configuration (defaults shown)
TF_VAR_email_host=smtp.gmail.com
TF_VAR_email_port=587 